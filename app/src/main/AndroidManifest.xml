<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nikitha.android.vizagtourguide">

<!--    <meta-data tools:replace="android:value" android:name="com.google.ar.core.min_apk_version" android:value="19" />-->

    <meta-data android:name="com.google.ar.core" android:value="optional" />
    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".MapImage"
            android:label="@string/vizagMap"
            android:parentActivityName=".MapActivity"></activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/map"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LocalFavActivity"
            android:label="@string/placestoVisit"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".HistoricPlacesActivity"
            android:label="@string/historicalPlaces"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CoffeeShopsActivity"
            android:label="@string/cofeeShops"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StreetFoodActivity"
            android:label="@string/localSteetfood"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>